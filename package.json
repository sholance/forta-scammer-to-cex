{
  "name": "cex-bot",
  "displayName": "CEX SCAM BOT",
  "version": "0.0.2",
  "description": "Forta Bot for detecting CEX scam deposit",
  "longDescription": "The bot detects CEX scam deposit by checking if the deposit address is a known scam address. The bot uses the following API to check if the address is a known scam address:",
  "repository": "https://github.com/sholance/cex-bot",
  "chainIds": [
    1,
    10,
    56,
    137,
    250,
    42161,
    43114
  ],
  "scripts": {
    "build": "tsc",
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e js,ts,json  --exec \"npm run build && forta-agent run\"",
    "start:prod": "forta-agent run --prod",
    "tx": "npm run build && forta-agent run --tx",
    "block": "npm run build && forta-agent run --block",
    "range": "npm run build && forta-agent run --range",
    "file": "npm run build && forta-agent run --file",
    "publish": "forta-agent publish",
    "push": "forta-agent push",
    "disable": "forta-agent disable",
    "enable": "forta-agent enable",
    "keyfile": "forta-agent keyfile",
    "test": "jest --detectOpenHandles",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "axios": "^1.5.0",
    "csv-parse": "^5.5.2",
    "dotenv": "^16.0.3",
    "forta-agent": "^0.1.36",
    "forta-helpers": "^1.0.9",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.8"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.198",
    "@types/node-fetch": "^2.6.2",
    "@types/nodemon": "^1.19.0",
    "forta-agent-tools": "^3.2.5",
    "jest": "^27.5.1",
    "nodemon": "^2.0.8",
    "prettier": "^2.8.4",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4"
  }
}
